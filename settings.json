{
  "editor.fontFamily": "JetBrains Mono, FiraCode Nerd Font Mono, Consolas, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "files.autoSave": "onFocusChange",
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit",
    "source.fixAll.eslint.format": "explicit",
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "editor.wordWrap": "on",
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
  "editor.bracketPairColorization.enabled": true,
  "editor.guides.bracketPairs": true,
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorBlinking": "smooth",
  "diffEditor.codeLens": true,
  "editor.formatOnPaste": true,
  "debug.console.fontSize": 13,
  "editor.tabSize": 2,
  "editor.minimap.autohide": "mouseover",
  "window.zoomLevel": 0,
  "editor.cursorStyle": "underline",
  "editor.smoothScrolling": true,
  "workbench.list.smoothScrolling": true,
  "terminal.integrated.smoothScrolling": true,
  "editor.fontSize": 14,
  "workbench.startupEditor": "none",
  "[xml]": {
    "editor.defaultFormatter": "mikeburgh.xml-format"
  },
  "workbench.editorLargeFileConfirmation": 10024,
  "workbench.editor.showTabs": "single",
  "workbench.tree.indent": 16,
  "workbench.editor.empty.hint": "hidden",
  "breadcrumbs.enabled": false,
  "breadcrumbs.symbolPath": "off",
  "breadcrumbs.filePath": "on",
  "editor.fontWeight": 450,
  "editor.lineNumbers": "on",
  "editor.folding": true,
  "editor.glyphMargin": true,
  "editor.guides.indentation": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "editor.unicodeHighlight.invisibleCharacters": false,
  "editor.stickyScroll.enabled": true,
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.cursorBlinking": true,
  "workbench.iconTheme": "vs-seti-folder",
  "workbench.colorTheme": "Islands Dark",
  "security.workspace.trust.untrustedFiles": "open",
  "window.menuBarVisibility": "compact",
  "workbench.navigationControl.enabled": false,
  "window.commandCenter": false,
  "remote.SSH.remotePlatform": {
    "10.20.90.71": "linux"
  },
  "workbench.sideBar.location": "right",
  "github.copilot.nextEditSuggestions.enabled": true,
  "gitlens.ai.model": "vscode",
  "gitlens.ai.vscode.model": "copilot:gpt-4.1",
  "chat.mcp.gallery.enabled": true,
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono",
  "custom-ui-style.font.monospace": "IBM Plex Mono",
  "editor.lineHeight": 1.8,
  "workbench.tree.renderIndentGuides": "always"
}
